   "source": [
    "from PIL import Image\n",
    "from glob import glob\n",
    "\n",
    "files=glob('./images/*.jpg')\n",
    "\n",
    "\n",
    "for file in files:\n",
    "    img = Image.open(file)\n",
    "    new_f=file.replace('./images\\\\', './images/res/')\n",
    "\n",
    "    resize_img = img.resize((800, 800))\n",
    "    resize_img.save(new_f)"
   ]
