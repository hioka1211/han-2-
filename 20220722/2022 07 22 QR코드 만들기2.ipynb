    "import qrcode \n",
    "#리스트 위한 간소화 하기\n",
    "naver = \"www.naver.com\"\n",
    "daum = \"www.daum.com\"\n",
    "google = \"google.com\"\n",
    "\n",
    "#리스트 하기\n",
    "url_list = [naver, daum, google]\n",
    "\n",
    "#체크하기\n",
    "print(url_list)\n",
    "\n",
    "#for 이용해서 코드 만들기\n",
    "for url in url_list:\n",
    "    img = qrcode.make(url)\n",
    "    img.save(\"C:/Users/user/Desktop/study/2차/link.png\")\n"
